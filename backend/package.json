{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "./server.js",
  "type": "module",
  "scripts": {
    "start": "cross-env DB='HOST' tsx ./server.ts",
    "start-local": "cross-env DB='LOCAL' tsx watch ./server.ts",
    "migration:create": "typeorm migration:create",
    "migration:create:name": "tsx ./src/db/migrations/helpers/create.migration.helper.ts",
    "migration:run": "cross-env DB='LOCAL' LOGGER='TRUE' tsx ./node_modules/typeorm/cli.js migration:run --dataSource ./src/db/database.service.ts",
    "migration:run:prod": "cross-env DB='HOST' LOGGER='TRUE' tsx ./node_modules/typeorm/cli.js migration:run --dataSource ./src/db/database.service.ts",
    "migration:revert": "cross-env LOGGER='TRUE' tsx ./node_modules/typeorm/cli.js migration:revert --dataSource ./src/db/database.service.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "eslint": "^8.51.0",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.6.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.20.6",
    "typeorm": "^0.3.27",
    "typescript": "^5.2.2",
    "typescript-ioc": "^3.2.2",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.19",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5"
  }
}
